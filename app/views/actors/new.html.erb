<h1>nuevo actor</h1>
<%= form_for @actor do |f| %>
  <%=  f.text_field :name %>
  <%=  f.text_field :bio %>
  <%=  f.text_field :birth_date %>
  <%=  f.text_field :birth_place %>
  <%=  f.text_field :image_url %>
  <%=  f.text_field :alive %>
  <%=  f.text_field :death_place %>
  <%=  f.text_field :death_date %>
  <%= f.submit %>
<% end %>
